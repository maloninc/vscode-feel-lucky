{
    "name": "feel-lucky",
    "displayName": "Feel Lucky",
    "description": "Open a random file from the selected folder.",
    "version": "0.0.1",
    "publisher": "maloninc",
    "repository": {
        "type": "git",
        "url": "https://github.com/maloninc/vscode-feel-lucky.git"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:feel-lucky.openRandomFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "feel-lucky.openRandomFile",
                "title": "Feel Lucky"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "feel-lucky.openRandomFile",
                    "when": "explorerResourceIsFolder",
                    "group": "navigation"
                }
            ]
        },
        "keybindings": [
            {
                "command": "feel-lucky.openRandomFile",
                "key": "ctrl+shift+r",
                "mac": "cmd+shift+r"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/node": "^14.14.37",
        "typescript": "^4.2.4"
    }
}